#: __init__.py:63 __init__.py:317 __init__.py:394
msgid "xPlorer"
msgstr "xPlorer"

#: __init__.py:246
msgid "Copied: {path}"
msgstr "Скопировано: {path}"

#: __init__.py:248 __init__.py:471
msgid "Could not open clipboard"
msgstr "Не удалось открыть буфер обмена"

#: __init__.py:250
msgid "Unable to get current path"
msgstr "Не удалось получить текущий путь"

#: __init__.py:316
msgid "Copy selected names (single tap) or copy address bar (double tap)"
msgstr "Копировать имена выбранных объектов (одно нажатие) или путь из адресной строки (два нажатия)"

#: __init__.py:356
msgid "Copy content (single tap) or invert selection (double tap)"
msgstr "Копировать содержимое (одно нажатие) или инвертировать выделение (два нажатия)"

#: __init__.py:393
msgid "Say size (single tap) or compress zip (double tap)"
msgstr "Озвучить размер (одно нажатие) или сжать в ZIP (два нажатия)"

#: __init__.py:410
msgid "Open xPlorer context menu"
msgstr "Открыть контекстное меню xPlorer"

#: __init__.py:425
msgid "Rename selected file"
msgstr "Переименовать выбранный файл"

#: __init__.py:435
msgid "Cannot open settings dialog"
msgstr "Не удалось открыть окно настроек"

#: __init__.py:441
msgid "Error opening settings"
msgstr "Ошибка при открытии настроек"

#: clipboardManager.py:30 clipboardManager.py:53
msgid "No items selected"
msgstr "Объекты не выбраны"

#: clipboardManager.py:46
msgid "Copied 1 item name to clipboard"
msgstr "Имя 1 объекта скопировано в буфер обмена"

#: clipboardManager.py:48
msgid "Copied {count} item names to clipboard"
msgstr "Имена {count} объектов скопированы в буфер обмена"

